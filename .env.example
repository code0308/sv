# Port where the Express server will run
PORT=3000

# MongoDB connection URI (e.g., MongoDB Atlas or local)
MONGO_URI=mongodb://localhost:27017/my_app

# Secret key to sign access JWTs (short-term)
JWT_SECRET=aSuperSecureSecretKey

# Secret key to sign refresh JWTs (long-term)
JWT_REFRESH_SECRET=anotherDifferentSecureKey